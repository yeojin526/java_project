package sec02.exam01;

public class IntegerLiteralEx {

	public static void main(String[] args) {
		byte var1 = -128;
		byte var2 = -129; //byte에 저장되는 값의 허용 범위: -128~127
		
		long var3 = 2150000000000000L;
		int var4 = 2150000000; // 21억5천 => int는 21억4천어쩌구~값까지만 허용됨
		
		char var11 = 'a';
		char var22 = '안'; //(유니코드)문자 - 한글도 가능!
		
		int var23 = '각'; //아스키코드값으로 숫자가 출력된다.
		
		String var33 = "안녕하세요"; //문자열 = 문자+문자
		
	}

}
