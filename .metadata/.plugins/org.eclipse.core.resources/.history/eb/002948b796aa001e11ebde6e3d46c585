package practice0104;

import java.util.Scanner;

public class PtTest1 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		System.out.print("세자리 수를 입력하시오: ");
		String num1 = sc.nextLine();
		System.out.print("세자리 수를 입력하시오: ");
		String num2 = sc.nextLine();
		
		String num2_1 = num2.substring(num2.length()-1,num2.length());
		String num2_2 = num2.substring(num2.length()-2,num2.length()-1);
		String num2_3 = num2.substring(num2.length()-3,num2.length()-2);
		
		int value1 = Integer.parseInt(num1);
		int value2 = Integer.parseInt(num2);
		
		System.out.println(" "+num1);
		System.out.println("X"+num2);
		System.out.println("-----------------");

		
		
		
		
		System.out.println("-----------------");
		System.out.println(value1*value2);	
	}
}
