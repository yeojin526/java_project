package sec02.exam01;

public class MethodEx {
	public static void method() { // 메서드 정의
		System.out.println("메서드(함수) method");
	}


	public static int method2() {
		return 0;
		
	}	
	public static void main(String[] args) {
		method(); // 호출
		int ret = method2();
		
		System.out.println(ret);

	}
}




