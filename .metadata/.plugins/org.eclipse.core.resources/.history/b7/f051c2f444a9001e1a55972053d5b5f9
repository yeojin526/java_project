package sec03.exam01;

import java.util.Scanner;

public class Studentcredits {
	public static void main(String[] args) {
		int count = 5;
		int[] num;
		int numMax =100;
		int numMin =0;
		
		Scanner sc = new Scanner(System.in);
		System.out.print("학점을 입력하시오:");
		count = sc.nextInt();
		num = new int[count];
		
		System.out.print(count+" 개의 숫자를 입력하세요!: ");
		for(int i=0; i<count; i++) {
			num[i]=sc.nextInt();
		}
		
		for(int i=0;i<count; i++)
			if (90<=num[i]&&num[i]<=100) {
			System.out.println("학점은 A");
			} else if(80<=num[i]&&num[i]<90) {
			System.out.println("학점은 B");
			} else if(70<=num[i]&&num[i]<80) {
			System.out.println("학점은 C");
		} else if(num[i]<70) {
			System.out.println("학점은 F");
		}
		
		
		
		//point[i]

		//for(int i=0; i<size; i++) {
		//System.out.println("점수를 입력하시오.");
		//point[i]= sc.next();
		}
		//System.out.println("학생의 학점은: "+ point[i])
	}


