package sec03.exam01;

public class Sum {

	public static void main(String[] args) {
		int sum = 0; //변수 sum은 계산하는 데 쓴다.
		
		for(int i=1; i<10; i++) {
			System.out.println("i:" + i);
			sum += i;
			//sum = sum + i;
			System.out.println("sum:" + sum);
	}
		System.out.println("1부터 10까지의 합계:"+sum);
	}
}

